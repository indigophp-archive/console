{
    "name": "indigophp/console",
    "description": "Console built with separated CLI components",
    "license": "MIT",
    "keywords": ["console", "climate"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "league/climate": "~3.0",
        "league/event": "~2.0",
        "league/booboo": "~1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Indigo\\Console\\Stub\\": "stub/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
